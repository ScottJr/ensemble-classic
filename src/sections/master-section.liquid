  | img_url: '800x' %}

{% assign section_bg_image_url_lg = section.settings.section_bg_image_lg
  | img_url: '1400x' %}  

{% comment %} END - Section Settings {% endcomment %}

{% comment %} START - Content Header {% endcomment %}
{% comment %} Content Header Settings {% endcomment %}
{% assign header_container = section.settings.header_container %}

{% assign header_col = section.settings.header_col %}
{% assign header_col_offset = section.settings.header_col_offset %}
{% assign header_col_lg = section.settings.header_col_lg  %}
{% assign header_col_offset_lg = section.settings.header_col_offset_lg  %}

{% assign header_pt = section.settings.header_pt %}
{% assign header_pb = section.settings.header_pb %}
{% assign header_pt_md_up = section.settings.header_pt_md_up %}
{% assign header_pb_md_up = section.settings.header_pb_md_up %}

{% assign header_mt = section.settings.header_mt %}
{% assign header_mb = section.settings.header_mb %}
{% assign header_mt_md_up = section.settings.header_mt_md_up %}
{% assign header_mb_md_up = section.settings.header_mb_md_up %}

{% assign header_bg_color = section.settings.header_bg_color %}
{% assign header_bg_color_alpha = section.settings.header_bg_color_alpha %}

{% assign header_content_color = section.settings.header_content_color %}

{% assign header_content_align = section.settings.header_content_align %}
{% assign header_content_align_lg = section.settings.header_content_align_lg %}

{% assign header_eyebrow_style = section.settings.header_eyebrow_style %}
{% assign header_eyebrow_text = section.settings.header_eyebrow_text %}

{% assign header_heading_style = section.settings.header_heading_style %}
{% assign header_heading_text = section.settings.header_heading_text %}

{% assign header_paragraph_text = section.settings.header_paragraph_text %}

