        "default": "FF",
        "info": "Default: Transparency Disabled"
      },
      {
        "type": "header",
        "content": "Small Devices",
        "info": "Screen sizes smaller than 768px"
      },
      {
        "type": "image_picker",
        "id": "section_bg_image",
        "label": "Background Image"
      },
      {
        "type": "header",
        "content": "Padding"
      },
      {
        "type":   "range",
        "label": "Top",
        "id": "section_pt",
        "min":       0,
