{% assign footer_heading_style = section.settings.footer_heading_style %}
{% assign footer_heading_text = section.settings.footer_heading_text %}

{% assign footer_paragraph_text = section.settings.footer_paragraph_text %}

{% assign footer_link_text  = section.settings.footer_link_text %}
{% assign footer_link_url = section.settings.footer_link_url %}
{% assign footer_link_style = section.settings.footer_link_style %}

{% comment %} Footer Content {% endcomment %}
{% capture content_footer %}
  {% render 'content-footer',
    container: footer_container,
    col: footer_col,
    col_offset: footer_col_offset,
    col_lg: footer_col_lg,
    col_offset_lg: footer_col_offset_lg,
    pt: footer_pt,
    pb: footer_pb,
    mt: footer_mt,
    mb: footer_mb,
    pt_md_up: footer_pt_md_up,
    pb_md_up: footer_pb_md_up,
    mt_md_up: footer_mt_md_up,
    mb_md_up: footer_mb_md_up,
    bg_color: footer_bg_color,
    bg_color_alpha: footer_bg_color_alpha,
    content_color: footer_content_color,
    content_align: footer_content_align,
    content_align_lg: footer_content_align_lg,
    eyebrow_style: footer_eyebrow_style,
    eyebrow_text: footer_eyebrow_text,
    heading_style: footer_heading_style,
    heading_text: footer_heading_text,
    paragraph_text: footer_paragraph_text,
    link_text: footer_link_text,
    link_url: footer_link_url,
    link_style: footer_link_style %}
{% endcapture %}

{% comment %} END - Footer {% endcomment %}

{% comment %} Render Section {% endcomment %}
{% render 'section',
  content_header: content_header,
  content: content,
  content_footer: content_footer,
  bg_color: section_bg_color,
  bg_color_alpha: section_bg_color_alpha,
  bg_image_url: section_bg_image_url,
