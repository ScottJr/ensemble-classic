{% comment %} START - Section Settings {% endcomment %}
{% assign section_type = 'master-section' %}

{% assign section_pt = section.settings.section_pt %}
{% assign section_pb = section.settings.section_pb %}
{% assign section_pt_md_up = section.settings.section_pt_md_up %}
{% assign section_pb_md_up = section.settings.section_pb_md_up %}

{% assign section_mt = section.settings.section_mt %}
{% assign section_mb = section.settings.section_mb %}
{% assign section_mt_md_up = section.settings.section_mt_md_up %}
