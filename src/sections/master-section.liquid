{% comment %} Content Header Capture {% endcomment %}
{% capture content_header %}
  {% render 'content-header'
    container: header_container,
    col: header_col,
    col_offset: header_col_offset,
    col_lg: header_col_lg,
    col_offset_lg: header_col_offset_lg
    pt: header_pt,
    pb: header_pb,
    mt: header_mt,
    mb: header_mb,
    pt_md_up: header_pt_md_up,
    pb_md_up: header_pb_md_up,
    mt_md_up: header_mt_md_up,
    mb_md_up: header_mb_md_up,
    bg_color: header_bg_color,
    bg_color_alpha: header_bg_color_alpha,
    content_color: header_content_color,
    content_align: header_content_align,
    content_align_lg: header_content_align_lg,
    eyebrow_style: header_eyebrow_style,
    eyebrow_text: header_eyebrow_text,
    heading_style: header_heading_style,
    heading_text: header_heading_text,
    paragraph_text: header_paragraph_text,
    link_text: header_link_text,
    link_url: header_link_url,
    link_style: header_link_style %}
{% endcapture %}
{% comment %} END - Header {% endcomment %}

{% comment %} START - Content {% endcomment %}
{% comment %} Content Settings {% endcomment %}
{% assign content_container = section.settings.content_container %}

{% assign content_row_no_gutters = section.settings.no_gutters %}

{% assign content_container_pt = section.settings.content_container_pt %}
{% assign content_container_pb = section.settings.content_container_pb %}
{% assign content_container_pt_md_up = section.settings.content_container_pt_md_up %}
{% assign content_container_pb_md_up = section.settings.content_container_pb_md_up %}

{% assign content_container_mt = section.settings.content_container_mt %}
{% assign content_container_mb = section.settings.content_container_mb %}
{% assign content_container_mt_md_up = section.settings.content_container_mt_md_up %}
{% assign content_container_mb_md_up = section.settings.content_container_mb_md_up %}

{% assign content_container_bg_color = section.settings.content_container_bg_color %}
{% assign content_container_bg_color_alpha = section.settings.content_container_bg_color_alpha %}

{% comment %} Content {% endcomment %}
{% capture section_blocks %}
  {% for block in section.blocks %}
    {% comment %} Defaults {% endcomment %}
    {% assign content = blank %}

    {% comment %} Global Block Settings {% endcomment %}
    {% assign col = block.settings.col %}
    {% assign col_offset = block.settings.col_offset %}
    {% assign col_lg = block.settings.col_lg  %}
    {% assign col_lg_offset = block.settings.col_lg_offset  %}

    {% if block.type == 'content-object' %}
      {% comment %} START - Content Grid Item {% endcomment %}

      {% comment %} Block Settings {% endcomment %}
      {% assign id = block.id %}

      {% assign aspect_ratio = block.settings.aspect_ratio %}
      {% assign fill_container = block.settings.fill_container %}

      {% assign bg_color = block.settings.bg_color %}
      {% assign bg_color_alpha = block.settings.bg_color_alpha %}

      {% assign bg_image_url = block.settings.bg_image
        | img_url: '400x'  %}
      {% assign bg_image_url_lg = block.settings.bg_image_lg
        | img_url: '400x' %}

      {% assign eyebrow_style = block.settings.eyebrow_style %}
      {% assign eyebrow_text = block.settings.eyebrow_text %}

      {% assign heading_style = block.settings.heading_style %}
      {% assign heading_text = block.settings.heading_text %}

      {% assign paragraph_text = block.settings.paragraph_text %}

      {% assign link_text  = block.settings.link_text %}
      {% assign link_url = block.settings.link_url %}
      {% assign link_style = block.settings.link_style %}

      {% assign content_color = block.settings.content_color %}
      {% assign content_align = block.settings.content_align %}
      {% assign content_align_lg = block.settings.content_align_lg %}

      {% assign align_items = block.settings.align_items %}

      {% assign pt = block.settings.pt %}
      {% assign pr = block.settings.pr %}
      {% assign pb = block.settings.pb %}
      {% assign pl = block.settings.pl %}

      {% assign mt = block.settings.mt %}
      {% assign mr = block.settings.mr %}
      {% assign mb = block.settings.mb %}
      {% assign ml = block.settings.ml %}

      {% assign pt_md_up = block.settings.pt_md_up %}
      {% assign pr_md_up = block.settings.pr_md_up %}
      {% assign pb_md_up = block.settings.pb_md_up %}
      {% assign pl_md_up = block.settings.pl_md_up %}

      {% assign mt_md_up = block.settings.mt_md_up %}
      {% assign mr_md_up = block.settings.mr_md_up %}
      {% assign mb_md_up = block.settings.mb_md_up %}
      {% assign ml_md_up = block.settings.ml_md_up %}

      {% comment %} Block Content {% endcomment %}
      {% capture content_object %}
        {% render 'content-object',
          content_color: content_color,
          eyebrow_style: eyebrow_style,
          eyebrow_text: eyebrow_text,
          heading_style: heading_style,
          heading_text: heading_text,
          paragraph_text: paragraph_text,
          link_text: link_text,
          link_url: link_url,
          link_style: link_style,
          text_align: content_align,
          text_align_lg: content_align_lg,
          pt: pt,
          pr: pr,
          pb: pb,
          pl: pl,
          mt: mt,
          mr: mr,
          mb: mb,
          ml: ml,
          pt_md_up: pt_md_up,
          pr_md_up: pr_md_up,
          pb_md_up: pb_md_up,
          pl_md_up: pl_md_up,
          mt_md_up: mt_md_up,
          mr_md_up: mr_md_up,
          mb_md_up: mb_md_up,
          ml_md_up: ml_md_up %}
      {% endcapture %}

      {% unless aspect_ratio == 'none' %}
        {% capture content %}
          {% render 'embed-object',
            content: content_object,
            aspect_ratio: aspect_ratio,
            fill_container: fill_container,
            bg_image_overlay: bg_image_overlay
            bg_color: bg_color,
            bg_color_alpha: bg_color_alpha,
            id: id,
            bg_image_url: bg_image_url,
            bg_image_url_lg: bg_image_url_lg,
            align_items: align_items %}
        {% endcapture %}
      {% endunless %}

      {% comment %} END - Content Grid Item {% endcomment %}
    {% elsif block.type == 'article-grid-item' %}
      {% comment %} START - Article Grid Item {% endcomment %}

      {% comment %} Block Settings {% endcomment %}
      {% assign article =  articles[block.settings.article] %}

      {% comment %} Block Content {% endcomment %}
      {% capture content %}
        {% render 'article-card',
          article: article,
          show_excerpt: true %}
      {% endcapture %}

      {% comment %} END - Article Grid Item {% endcomment %}
    {% elsif block.type == 'content-card' %}
      {% comment %} START - Content Card Grid Item {% endcomment %}
      {% assign content_align = block.settings.content_align %}
      {% assign content_align_lg = block.settings.content_align_lg %}
      {% assign content_color = block.settings.content_color %}

      {% assign eyebrow_text = block.settings.eyebrow_text %}
      {% assign eyebrow_style = block.settings.eyebrow_style %}

      {% assign heading_text = block.settings.heading_text %}
      {% assign heading_style = block.settings.heading_style %}
      {% assign heading_size = 'h3' %}

      {% assign paragraph_text = block.settings.paragraph_text %}

      {% assign link_text = block.settings.link_text %}
      {% assign link_url = block.settings.link_url %}
      {% assign link_style = block.settings.link_style %}

      {% assign image_url = block.settings.image.src
        | img_url: '600x' %}
      {% assign image_url_lg = block.settings.image_lg.src
        | img_url: '1000x' %}
      {% assign image_aspect_ratio = block.settings.aspect_ratio %}
      {% assign image_alt_text = block.settings.image_alt_text %}

      {% assign pt = block.settings.pt %}
      {% assign pr = block.settings.pr %}
      {% assign pb = block.settings.pb %}
      {% assign pl = block.settings.pl %}

      {% assign mt = block.settings.mt %}
      {% assign mr = block.settings.mr %}
      {% assign mb = block.settings.mb %}
      {% assign ml = block.settings.ml %}

      {% assign pt_md_up = block.settings.pt_md_up %}
      {% assign pr_md_up = block.settings.pr_md_up %}
      {% assign pb_md_up = block.settings.pb_md_up %}
      {% assign pl_md_up = block.settings.pl_md_up %}

      {% assign mt_md_up = block.settings.mt_md_up %}
      {% assign mr_md_up = block.settings.mr_md_up %}
      {% assign mb_md_up = block.settings.mb_md_up %}
      {% assign ml_md_up = block.settings.ml_md_up %}

      {% assign bg_color = block.settings.bg_color %}
      {% assign bg_color_alpha = block.settings.bg_color_alpha %}

      {% assign bg_image_url = block.settings.bg_image_url %}
      {% assign bg_image_url_lg = block.settings.bg_image_url_lg %}

      {% capture content %}
        {% render 'content-card'
          content_color: content_color,
          content_align: content_align,
          content_align_lg: content_align_lg,
          eyebrow_style: eyebrow_style,
          eyebrow_text: eyebrow_text,
          heading_text: heading_text,
          heading_size: heading_size,
          heading_style: heading_style,
          paragraph_text: paragraph_text,
          image_url: image_url,
          image_url_lg: image_url_lg,
          image_alt_text: image_alt_text,
          image_aspect_ratio: image_aspect_ratio,
          link_style: link_style,
          link_url: link_url,
          link_text: link_text,
          pt: pt,
          pr: pr,
          pb: pb,
          pl: pl,
          mt: mt,
          mr: mr,
          mb: mb,
          ml: ml,
          pt_md_up: pt_md_up,
          pr_md_up: pr_md_up,
          pb_md_up: pb_md_up,
          pl_md_up: pl_md_up,
          mt_md_up: mt_md_up,
          mr_md_up: mr_md_up,
          mb_md_up: mb_md_up,
          ml_md_up: ml_md_up,
          bg_image_url: bg_image_url,
          bg_image_url_lg: bg_image_url_lg,
          bg_color: bg_color,
          bg_color_alpha: bg_color_alpha %}
      {% endcapture %}

      {% comment %} END - Content Card Grid Item {% endcomment %}
    {% elsif block.type == 'product-grid-item' %}
      {% comment %} START - Product Grid Item {% endcomment %}

      {% comment %} Block Settings {% endcomment %}
      {% assign product = all_products[block.settings.product] %}
      {% assign featured_image = product.featured_image.src
        | img_url: '600x' %}
      {% assign product_excerpt = product.metafields.custom_fields["excerpt"] %}

      {% comment %} Block Content {% endcomment %}
      {% capture content %}
        {% render 'product-card',
          featured_image: featured_image,
          id: product.id,
          title: product.title,
          excerpt: product_excerpt,
          url: product.url,
          compare_at_price: product.compare_at_price,
          price_varies: product.price_varies,
          price: product.price %}
      {% endcapture %}

      {% comment %} END - Product Grid Item {% endcomment %}
    {% elsif block.type == 'image-grid-item' %}
      {% comment %} START - Image Grid Item {% endcomment %}

      {% comment %} Block Settings {% endcomment %}
      {% assign image_url = block.settings.image.src
        | img_url: '600x' %}
      {% assign image_url_lg = block.settings.image_lg
        | img_url: '1000x' %}
      {% assign image_aspect_ratio = block.settings.image_aspect_ratio %}
      {% assign image_alt_text = block.settings.image.alt %}

      {% comment %} Block Content {% endcomment %}
      {% capture content %}
        {% render 'image-object',
          image_url: image_url,
          image_url_lg: image_url_lg,
          image_alt_text: image_alt_text,
          aspect_ratio: image_aspect_ratio %}
      {% endcapture %}
      {% comment %} End - Image Grid Item {% endcomment %}
    {% elsif block.type == 'video-grid-item' %}
      {% comment %} START - Video Grid Item {% endcomment %}

      {% comment %} Block Settings {% endcomment %}
      {% assign video_url = block.settings.video_url %}
      {% assign video_aspect_ratio = block.settings.video_aspect_ratio %}

      {% comment %} Block Content {% endcomment %}
      {% capture content %}
        {% render 'video-object',
          video_url: video_url,
          video_aspect_ratio: video_aspect_ratio %}
      {% endcapture %}

      {% comment %} END - Video Grid Item {% endcomment %}
    {% endif %}

    {% comment %} Render Grid Item With Embedded Content {% endcomment %}
    {% if content != blank %}
      {% render 'grid-item',
        col: col,
        col_offset: col_offset,
        col_lg: col_lg,
        col_lg_offset: col_lg_offset,
        gutters: content_row_no_gutters,
        content: content %}
    {% endif %}
  {% endfor %}
{% endcapture %}

{% capture content %}
  {% render 'content-container',
    content: section_blocks,
    container: content_container,
    row_no_gutters: content_row_no_gutters,
    pt: content_container_pt,
    pb: content_container_pb,
    pt_md_up: content_container_pt_md_up,
    pb_md_up: content_container_pb_md_up,
    mt: content_container_mt,
    mb: content_container_mb,
    mt_md_up: content_container_mt_md_up,
    mb_md_up: content_container_mb_md_up,
    bg_color: content_container_bg_color,
    bg_color_alpha: content_container_bg_color_alpha %}
{% endcapture %}
{% comment %} END - Content {% endcomment %}

{% comment %} START - Footer {% endcomment %}
{% comment %} Content Footer Settings {% endcomment %}
{% assign footer_container = section.settings.footer_container %}

{% assign footer_col = section.settings.footer_col %}
{% assign footer_col_lg = section.settings.footer_col_lg  %}

{% assign footer_pt = section.settings.footer_pt %}
{% assign footer_pb = section.settings.footer_pb %}
{% assign footer_mt = section.settings.footer_mt %}
{% assign footer_mb = section.settings.footer_mb %}

{% assign footer_pt_md_up = section.settings.footer_pt_md_up %}
