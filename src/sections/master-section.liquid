      },
      {
        "type":   "range",
        "label": "Bottom",
        "id": "section_pb",
        "min":       0,
        "max":       5,
        "step":      1,
        "default":   4,
        "unit": "%"
      },
      {
        "type": "header",
        "content": "Margin"
      },
      {
        "type":   "range",
        "label": "Top",
        "id": "section_mt",
        "min":       0,
        "max":       5,
        "step":      1,
        "default":   0,
        "unit": "%"
      },
      {
        "type":   "range",
        "label": "Bottom",
        "id": "section_mb",
        "min":       0,
        "max":       5,
        "step":      1,
        "default":   0,
        "unit": "%"
      },
      {
        "type": "header",
        "content": "Medium & Up Devices",
        "info": "Screen sizes larger than 768px"
      },
      {
        "type": "image_picker",
        "id": "section_bg_image_lg",
        "label": "Background Image"
      },
      {
        "type": "header",
        "content": "Padding"
      },
      {
        "type":   "range",
        "label": "Top",
        "id": "section_pt_md_up",
        "min":       0,
        "max":       5,
        "step":      1,
        "default":   4,
        "unit": "%"
      },
      {
        "type":   "range",
        "label": "Bottom",
        "id": "section_pb_md_up",
        "min":       0,
        "max":       5,
        "step":      1,
        "default":   4,
        "unit": "%"
      },
      {
        "type": "header",
        "content": "Margin"
