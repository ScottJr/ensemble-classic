// Container
.#{$ec-prefix}row {
  display: flex;
  flex: 1 0 100%;
  flex-wrap: wrap;
  margin-top: $gutter-y * -1;
  margin-right: $gutter-x / -2;
  margin-left: $gutter-x / -2;
}

.#{$ec-prefix}no-gutters {
  margin-right: 0;
  margin-left: 0;

  > .#{$ec-prefix}col,
  > [class*="#{$ec-prefix}col-"] {
    padding-right: 0;
    padding-left: 0;
  }
}

.#{$ec-prefix}container,
.#{$ec-prefix}container-fluid,
.#{$ec-prefix}container-sm,
.#{$ec-prefix}container-md,
.#{$ec-prefix}container-lg,
.#{$ec-prefix}container-xl,
.#{$ec-prefix}container-xxl {
  width: 100%;
