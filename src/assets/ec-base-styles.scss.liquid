// Variables
//
//

$ec-prefix: {{ settings.ec_prefix }} !default;

// Color system

$white: #fff !default;
$gray-100: #f8f9fa !default;
$gray-200: #e9ecef !default;
$gray-300: #dee2e6 !default;
$gray-400: #ced4da !default;
$gray-500: #adb5bd !default;
$gray-600: #6c757d !default;
$gray-700: #495057 !default;
$gray-800: #343a40 !default;
$gray-900: #212529 !default;
$black: #000 !default;

$blue: #0d6efd !default;
$indigo: #6610f2 !default;
$purple: #6f42c1 !default;
$pink: #d63384 !default;
$red: #dc3545 !default;
$orange: #fd7e14 !default;
$yellow: #ffc107 !default;
$green: #198754 !default;
$teal: #20c997 !default;
$cyan: #0dcaf0 !default;
{% comment %} Theme Overrides {% endcomment %}
{% if settings.primary_color != blank %}
  $primary: {{ settings.primary_color }};
{% else %}
  $primary: $blue !default;
{% endif %}

{% if settings.secondary_color != blank %}
  $secondary: {{ settings.secondary_color }};
{% else %}
  $secondary: $gray-600 !default;
{% endif %}

{% if settings.success_color != blank %}
  $success: {{ settings.success_color }};
{% else %}
  $success: $green !default;
{% endif %}

{% if settings.info_color != blank %}
  $info: {{ settings.info_color }};
{% else %}
  $info: $cyan !default;
{% endif %}

{% if settings.warning_color != blank %}
  $warning: {{ settings.warning_color }};
{% else %}
  $warning: $yellow !default;
{% endif %}

{% if settings.danger_color != blank %}
  $danger: {{ settings.danger_color }};
{% else %}
  $danger: $red !default;
{% endif %}

{% if settings.light_gray_color != blank %}
  $light: {{ settings.light_gray_color }};
{% else %}
  $light: $gray-100 !default;
{% endif %}

{% if settings.dark_gray_color != blank %}
  $dark: {{ settings.dark_gray_color }};
{% else %}
  $dark: $gray-800 !default;
{% endif %}

{% if settings.body_color != blank %}
  $body: {{ settings.body_color }};
{% else %}
  $body: $gray-900 !default;
{% endif %}

{% if settings.text_muted_color != blank %}
  $text-muted: {{ settings.text_muted_color }};
{% else %}
  $text-muted: $gray-600 !default;
{% endif %}

// Spacing
//
//

$spacer: 1rem !default;
$spacer-0: 0 !default;
$spacer-1: $spacer / 4 !default;
$spacer-2: $spacer / 2 !default;
$spacer-3: $spacer !default;
$spacer-4: $spacer * 1.5 !default;
$spacer-5: $spacer * 3 !default;
$spacer-6: $spacer * 4.5 !default;
$spacer-7: $spacer * 6 !default;
$spacer-8: $spacer * 7.5 !default;
$spacer-9: $spacer * 9 !default;
$spacer-10: $spacer * 10.5 !default;

// Grid breakpoints
//
//

$grid-breakpoint-xs: 0;
$grid-breakpoint-sm: {{ settings.ec_container_width_sm | append: 'px' }};
$grid-breakpoint-md: {{ settings.ec_container_width_md | append: 'px' }};
$grid-breakpoint-lg: {{ settings.ec_container_width_lg | append: 'px' }};
$grid-breakpoint-xl: {{ settings.ec_container_width_xl| append: 'px' }};
$grid-breakpoint-xxl: {{ settings.ec_container_width_xxl | append: 'px' }};

// Grid containers
//
//

$container-max-width-sm: {{ settings.ec_container_max_width_sm | append: 'px' }};
$container-max-width-md: {{ settings.ec_container_max_width_md | append: 'px' }};
$container-max-width-lg: {{ settings.ec_container_max_width_lg | append: 'px' }};
$container-max-width-xl: {{ settings.ec_container_max_width_xl | append: 'px' }};
$container-max-width-xxl: {{ settings.ec_container_max_width_xxl | append: 'px' }};

// Grid columns
//
//

$grid-gutter-width: $spacer-4 !default;

$gutter-x: $spacer-4;
$gutter-y: $spacer-0;

// Components
//
// Define common padding and border radius sizes and more.

$border-width: 1px !default;
$border-radius: {{ settings.ec_border_radius | append: 'px' }} !default;

// Typography
//
// Font, line-height, and color for body text, headings, and more.

$font-size-root: null !default;
$font-size-base: 1rem !default;
$font-size-sm: $font-size-base * 0.875 !default;
$font-size-lg: $font-size-base * 1.25 !default;

$font-weight-lighter: lighter !default;
$font-weight-light: 300 !default;
$font-weight-normal: 400 !default;
$font-weight-bold: 700 !default;
$font-weight-bolder: bolder !default;

$font-weight-base: $font-weight-normal !default;

$line-height-base: 1.5 !default;
$line-height-sm: 1.25 !default;
$line-height-lg: 2 !default;

$h1-font-size: $font-size-base * 2.5 !default;
$h2-font-size: $font-size-base * 2 !default;
$h3-font-size: $font-size-base * 1.75 !default;
$h4-font-size: $font-size-base * 1.5 !default;
$h5-font-size: $font-size-base * 1.25 !default;
$h6-font-size: $font-size-base !default;

$headings-margin-bottom: $spacer / 2 !default;
$headings-font-family: null !default;
$headings-font-style: null !default;
$headings-font-weight: 500 !default;
$headings-line-height: 1.2 !default;
$headings-color: null !default;

$display-font-size-1: 5rem !default;
$display-font-size-2: 4.5rem !default;
$display-font-size-3: 4rem !default;
$display-font-size-4: 3.5rem !default;
$display-font-size-5: 3rem !default;
$display-font-size-6: 2.5rem !default;

$display-font-weight: 300 !default;
$display-line-height: $headings-line-height !default;

// Buttons + Forms
//
// Shared variables that are reassigned to `$input-` and `$btn-` specific variables.

$input-btn-padding-y: 0.375rem !default;
$input-btn-padding-x: 0.75rem !default;
$input-btn-font-family: null !default;
$input-btn-font-size: $font-size-base !default;
$input-btn-line-height: $line-height-base !default;

$input-btn-focus-width: 0.2rem !default;
$input-btn-focus-box-shadow: 0 0 0 $input-btn-focus-width;

$input-btn-padding-y-sm: 0.25rem !default;
$input-btn-padding-x-sm: 0.5rem !default;
$input-btn-font-size-sm: $font-size-sm !default;

$input-btn-padding-y-lg: 0.5rem !default;
$input-btn-padding-x-lg: 1rem !default;
$input-btn-font-size-lg: $font-size-lg !default;

$input-btn-border-width: $border-width !default;

// Buttons
//
// For each of Bootstrap's buttons, define text, background, and border color.

$btn-padding-y: $input-btn-padding-y !default;
$btn-padding-x: $input-btn-padding-x !default;
$btn-font-family: $input-btn-font-family !default;
$btn-font-size: $input-btn-font-size !default;
$btn-line-height: $input-btn-line-height !default;
$btn-white-space: null !default;

$btn-padding-y-sm: $input-btn-padding-y-sm !default;
$btn-padding-x-sm: $input-btn-padding-x-sm !default;
$btn-font-size-sm: $input-btn-font-size-sm !default;

$btn-padding-y-lg: $input-btn-padding-y-lg !default;
$btn-padding-x-lg: $input-btn-padding-x-lg !default;
$btn-font-size-lg: $input-btn-font-size-lg !default;

$btn-border-width: $input-btn-border-width !default;

$btn-font-weight: $font-weight-normal !default;
$btn-focus-width: $input-btn-focus-width !default;
$btn-focus-box-shadow: $input-btn-focus-box-shadow !default;
$btn-disabled-opacity: 3 !default;
$btn-active-box-shadow: inset 0 3px 5px rgba($black, 0.125) !default;

$btn-block-spacing-y: 0.5rem !default;

$btn-border-radius: $border-radius !default;

// Badges

$badge-font-size: 0.75em !default;
$badge-font-weight: $font-weight-bold !default;
$badge-color: $white !default;
$badge-padding-y: 0.35em !default;
$badge-padding-x: 0.65em !default;
$badge-border-radius: $border-radius !default;

